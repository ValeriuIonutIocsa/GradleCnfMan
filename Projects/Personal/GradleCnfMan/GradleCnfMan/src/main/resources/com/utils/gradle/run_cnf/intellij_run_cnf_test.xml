<component name="ProjectRunConfigurationManager">
    <configuration default="false" name="@@CNF_NAME@@" type="JUnit" factoryName="JUnit" folderName="@@FOLDER_NAME@@"
                   nameIsGenerated="true">
        <module name="@@MODULE_NAME@@"/>
        <extension name="coverage">
            <pattern>
                <option name="PATTERN" value="@@PACKAGE_NAME@@.*"/>
                <option name="ENABLED" value="true"/>
            </pattern>
        </extension>
        <option name="PACKAGE_NAME" value="@@PACKAGE_NAME@@"/>
        <option name="MAIN_CLASS_NAME" value="@@MAIN_CLASS_NAME@@"/>
        <option name="METHOD_NAME" value="@@METHOD_NAME@@"/>
        <option name="TEST_OBJECT" value="method"/>
        <method v="2">
            <option name="MakeProject" enabled="true"/>
        </method>
    </configuration>
</component>